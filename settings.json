{
  //------------------------------------------------------------------------------------------------
  //Format on Save
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll": "explicit"
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  // "editor.defaultFormatter": "rvest.vs-code-prettier-eslint",
  // "editor.defaultFormatter": "dbaeumer.vscode-eslint",

  //Prettier Settings Global
  "prettier.endOfLine": "lf",
  "prettier.printWidth": 100,
  "prettier.trailingComma": "es5",
  "prettier.semi": false,
  "prettier.useEditorConfig": true,
  "prettier.singleQuote": true,
  //------------------------------------------------------------------------------------------------
  // General
  "workbench.startupEditor": "none",
  "explorer.compactFolders": false,
  "editor.renderLineHighlight": "all",
  "workbench.editor.labelFormat": "short",
  "extensions.ignoreRecommendations": true,
  "breadcrumbs.enabled": true,
  "editor.parameterHints.enabled": true,
  "explorer.confirmDragAndDrop": false,
  "explorer.confirmDelete": false,
  "editor.rulers": [80, 100, 120],
  "window.menuBarVisibility": "toggle",
  "editor.minimap.enabled": true,
  "editor.inlineSuggest.enabled": true,
  //------------------------------------------------------------------------------------------------
  "files.exclude": {
    "**/node_modules": true
  },
  //------------------------------------------------------------------------------------------------
  // Font
  "editor.tabSize": 2,
  "editor.fontSize": 14,
  "editor.lineHeight": 18,
  "editor.fontFamily": "Fira Code Retina",
  "editor.fontLigatures": true,
  //------------------------------------------------------------------------------------------------
  // Themes
  "workbench.iconTheme": "material-icon-theme",
  "workbench.colorTheme": "Dracula Pro (Van Helsing)",
  "material-icon-theme.activeIconPack": "nest",
  "workbench.productIconTheme": "fluent-icons",
  //------------------------------------------------------------------------------------------------
  //New Files
  "files.eol": "\n",
  "files.trimTrailingWhitespace": true,
  "files.insertFinalNewline": true,
  "security.workspace.trust.untrustedFiles": "newWindow",
  //------------------------------------------------------------------------------------------------
  // Brackets colorize
  "editor.semanticHighlighting.enabled": "configuredByTheme",
  "editor.bracketPairColorization.enabled": true,
  "editor.guides.bracketPairs": "active",
  //------------------------------------------------------------------------------------------------
  //Git
  "git.enableSmartCommit": false,
  "git.autofetch": true,
  "git.confirmSync": false,
  //------------------------------------------------------------------------------------------------
  //Typescript
  "typescript.tsserver.log": "off",
  //------------------------------------------------------------------------------------------------
  //Imports
  "javascript.suggest.autoImports": true,
  "typescript.suggest.autoImports": true,
  "typescript.updateImportsOnFileMove.enabled": "never",
  "javascript.updateImportsOnFileMove.enabled": "always",
  //------------------------------------------------------------------------------------------------
  // Emmets
  "emmet.syntaxProfiles": {
    "javascript": "jsx"
  },
  "emmet.includeLanguages": {
    "javascript": "javascriptreact"
  },
  //------------------------------------------------------------------------------------------------
  //------------------------------------------------------------------------------------------------
  //----------------------------------------Extensions----------------------------------------------
  //----------------------------------------Extensions----------------------------------------------
  //----------------------------------------Extensions----------------------------------------------
  //------------------------------------------------------------------------------------------------
  //------------------------------------------------------------------------------------------------
  //Material Icon Theme
  "material-icon-theme.folders.theme": "specific",
  "material-icon-theme.folders.associations": {
    "infra": "app",
    "entities": "class",
    "domain": "class",
    "schemas": "class",
    "typeorm": "database",
    "repositories": "mappings",
    "http": "container",
    "migrations": "tools",
    "modules": "components",
    "implementations": "core",
    "dtos": "typescript",
    "fakes": "mock",
    "websockets": "pipe",
    "protos": "pipe",
    "grpc": "pipe",
    "providers": "include",
    "subscribers": "messages",
    "useCases": "controller",
    "kafka": "scripts",
    "mappers": "meta",
    "_shared": "shared",
    "eslint-config": "tools",
    "kube": "kubernetes",
    "reducers": "Redux-reducer",
    "actions": "Redux-actions",
    "store": "Redux-store",
    "redux": "Redux-actions",
    "storage": "Temp",
    "presentation": "Mjml",
    "exceptions": "Error",
    "exception": "Error",
    "filters": "Private",
    "filter": "Private"
  },
  "material-icon-theme.files.associations": {
    "index.ts": "Minecraft",
    "index.js": "Minecraft",
    "styles.ts": "Figma",

    "*.webpack.js": "webpack",
    //
    "*.reducer.js": "Redux-reducer",
    "reducer.js": "Redux-reducer",
    "*.reducer.ts": "Redux-reducer",
    "reducer.ts": "Redux-reducer",
    //
    "*.action.js": "Redux-action",
    "action.js": "Redux-action",
    "*.action.ts": "Redux-action",
    "action.ts": "Redux-action",
    //
    "*.store.js": "Redux-store",
    "store.js": "Redux-store",
    "*.store.ts": "Redux-store",
    "store.ts": "Redux-store",
    //
    "*.i18n.js": "I18n",
    "*.i18n.ts": "I18n",
    //
    "routes.js": "Routing",
    "router.js": "Routing",
    "*router.js": "Routing",
    "*router.ts": "Routing",
    "routes.ts": "Routing",
    "router.ts": "Routing",
    "*.routes.ts": "Routing",
    "*.router.ts": "Routing",
    //
    "types.ts": "typescript-def",
    "type.ts": "typescript-def",
    "*.types.ts": "typescript-def",
    "*.type.ts": "typescript-def"
  },
  "material-icon-theme.languages.associations": {
    "dotenv": "tune"
  },

  // Reassociate files wih the correct language
  "files.associations": {
    ".sequelizerc": "javascript",
    ".stylelintrc": "json",
    ".prettierrc": "json",
    "*.tsx": "typescriptreact",
    ".env.*": "dotenv",
    ".env*": "dotenv"
  },

  //inlineparameters
  "workbench.colorCustomizations": {
    "inlineparameters.annotationForeground": "#8be9fd",
    "inlineparameters.annotationBackground": "#ffffff00"
  },

  //GitLens
  "gitlens.codeLens.recentChange.enabled": false,
  "gitlens.codeLens.authors.enabled": false,
  "gitlens.codeLens.enabled": false,

  //Colorize
  "colorize.include": ["*"],

  //TODO Tree
  "todo-tree.regex.regex": "(//|#|<!--|;|/\\*|^|^\\s*(-|\\d+.))\\s*($TAGS)",
  "todo-tree.general.tags": ["BUG", "HACK", "FIXME", "TODO", "XXX", "[ ]", "[x]", "TESTE"],

  //Template String Converter
  "template-string-converter.quoteType": "both",
  "template-string-converter.autoRemoveTemplateString": true,
  "template-string-converter.convertWithinTemplateString": true,
  "template-string-converter.addBracketsToProps": true,
  "template-string-converter.validLanguages": [
    "svelte",
    "typescript",
    "javascript",
    "typescriptreact",
    "javascriptreact"
  ],
  //Code Spell Checker
  "cSpell.language": "en,pt,pt_BR",
  "cSpell.userWords": [
    "airbnb",
    "backoffice",
    "camelcase",
    "chakra",
    "Cnpj",
    "dayjs",
    "defaulticon",
    "draftjs",
    "gatsby",
    "hookform",
    "Iphone",
    "jwplayer",
    "lightbox",
    "lottie",
    "mailtrap",
    "middlewares",
    "minetoo",
    "nestjs",
    "nextjs",
    "ongoingorder",
    "pageview",
    "Parens",
    "plopfile",
    "pokemon",
    "recaptcha",
    "reduxjs",
    "screenfull",
    "storaged",
    "stylelint",
    "svgr",
    "sxsw",
    "SXSW",
    "tailwindcss",
    "tanstack",
    "theming",
    "toastify",
    "trpc",
    "typecheck",
    "uuidv",
    "videojs",
    "vitejs",
    "Youtube"
  ],

  //GitHub Copilot
  "github.copilot.enable": {
    "*": true,
    "plaintext": true,
    "markdown": true,
    "scminput": false,
    "yaml": false,
    "typescriptreact": true,
    "typescript": true
  },
  "editor.accessibilitySupport": "off",
  "terminal.integrated.env.windows": {},
  "terminal.integrated.env.linux": {},
  "terminal.integrated.env.osx": {},
  "terminal.integrated.enableMultiLinePasteWarning": false,
  "remote.SSH.remotePlatform": {
    "Macbook": "macOS",
    "renans-mbp": "macOS"
  },
  "remote.SSH.connectTimeout": 30,
  "editor.tokenColorCustomizations": {
    "[*Light*]": {
      "textMateRules": [
        {
          "scope": "ref.matchtext",
          "settings": {
            "foreground": "#000"
          }
        }
      ]
    },
    "[*Dark*]": {
      "textMateRules": [
        {
          "scope": "ref.matchtext",
          "settings": {
            "foreground": "#fff"
          }
        }
      ]
    },
    "textMateRules": []
  },
  "[prisma]": {
    "editor.defaultFormatter": "Prisma.prisma"
  },
  "tailwindCSS.experimental.classRegex": [["tv\\(([^)]*)\\)", "[\"'`]([^\"'`]*).*?[\"'`]"]],
  "console-ninja.featureSet": "Community",
  "[svelte]": {
    "editor.defaultFormatter": "svelte.svelte-vscode"
  },
  "remote.autoForwardPortsSource": "hybrid",
  "svelte.enable-ts-plugin": true
  //------------------------------------------------------------------------------------------------
  //------------------------------------------------------------------------------------------------
  //------------------------------------------------------------------------------------------------
  //------------------------------------------------------------------------------------------------
  //------------------------------------------------------------------------------------------------
  //------------------------------------------------------------------------------------------------
  //------------------------------------------------------------------------------------------------
  //------------------------------------------------------------------------------------------------
  //------------------------------------------------------------------------------------------------
  //------------------------------------------------------------------------------------------------
  //------------------------------------------------------------------------------------------------
  //------------------------------------------------------------------------------------------------
  //------------------------------------------------------------------------------------------------
  //------------------------------------------------------------------------------------------------
  //------------------------------------------------------------------------------------------------
  //------------------------------------------------------------------------------------------------
  //------------------------------------------------------------------------------------------------
  //------------------------------------------------------------------------------------------------
}
