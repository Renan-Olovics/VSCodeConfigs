{
  //------------------------------------------------------------------------------------------------
  // VSCode settings
  // General
  "workbench.startupEditor": "none",
  "explorer.compactFolders": false,
  "editor.renderLineHighlight": "all",
  "workbench.editor.labelFormat": "short",
  "extensions.ignoreRecommendations": false,
  "breadcrumbs.enabled": true,
  "editor.parameterHints.enabled": true,
  "explorer.confirmDragAndDrop": false,
  "explorer.confirmDelete": false,
  "editor.rulers": [80, 100, 120],
  "screencastMode.onlyKeyboardShortcuts": true,
  "window.menuBarVisibility": "toggle",
  "editor.minimap.enabled": true,
  "editor.inlineSuggest.enabled": true,
  // Font
  "editor.tabSize": 2,
  "editor.fontSize": 14,
  "editor.lineHeight": 18,
  // "editor.fontFamily": "JetBrains Mono",
  "editor.fontFamily": "Fira Code Retina",
  "editor.fontLigatures": true,

  //New Files
  "files.eol": "\n",
  "files.trimTrailingWhitespace": true,
  "files.insertFinalNewline": true,
  "security.workspace.trust.untrustedFiles": "newWindow",

  // Brackets colorize
  "editor.semanticHighlighting.enabled": "configuredByTheme",
  "editor.bracketPairColorization.enabled": true,
  "editor.guides.bracketPairs": "active",

  // Themes
  "workbench.colorTheme": "Dracula Pro (Van Helsing)",
  "workbench.iconTheme": "material-icon-theme",
  "material-icon-theme.activeIconPack": "nest",
  "workbench.productIconTheme": "fluent-icons",

  //Terminal
  "terminal.integrated.fontSize": 14,
  "terminal.integrated.fontFamily": "Fira Code Retina",
  "terminal.integrated.defaultProfile.linux": "zsh",
  "terminal.integrated.defaultProfile.osx": "zsh",
  "terminal.integrated.defaultProfile.windows": "Git Bash",
  "terminal.integrated.showExitAlert": false,

  //Git
  "git.enableSmartCommit": false,
  "git.autofetch": true,
  "git.confirmSync": false,

  //Typescript
  "typescript.tsserver.log": "off",

  //Imports
  "javascript.suggest.autoImports": true,
  "typescript.suggest.autoImports": true,
  "typescript.updateImportsOnFileMove.enabled": "never",
  "javascript.updateImportsOnFileMove.enabled": "always",

  // Emmets
  "emmet.syntaxProfiles": {
    "javascript": "jsx"
  },
  "emmet.includeLanguages": {
    "javascript": "javascriptreact"
  },

  //------------------------------------------------------------------------------------------------
  //Format on Save
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll": "explicit"
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  // "editor.defaultFormatter": "rvest.vs-code-prettier-eslint",
  // "editor.defaultFormatter": "dbaeumer.vscode-eslint",

  // //Prettier Settings Global
  "prettier.endOfLine": "lf",
  "prettier.printWidth": 100,
  "prettier.trailingComma": "es5",
  "prettier.semi": false,
  "prettier.useEditorConfig": true,
  "prettier.singleQuote": true,

  //------------------------------------------------------------------------------------------------
  // Overwrite default settings

  //Files
  "files.exclude": {
    // ".vscode": true,
    // ".next": true,
    // "next-env.d.ts": true,
    // "next.config.js": true,
    // "tsconfig.json": true,
    // "config": true,
    // "**/.git": true,
    // "**/.svn": true,
    // "**/.hg": true,
    // "**/CVS": true,
    // "**/.DS_Store": true,
    // ".husky": true,
    // ".idea": true,
    // ".expo": true,
    // ".expo-shared": true,
    // ".editorconfig": true,
    // ".eslintignore": true,
    // ".gitignore": true,
    // ".eslintrc.js": true,
    // ".eslintrc.json": true,
    // ".prettierrc.js": true,
    // ".project": true,
    // "babel.config.js": true,
    // "commitlint.config.js": true,
    // "debug.log": true,
    // "metro.config.js": true,
    // "yarn.lock": true,
    // "yarn-error.log": true,
    // "README.md": true,
    // "**/node_modules": true
  },

  //Material Icon Theme
  "material-icon-theme.folders.theme": "specific",
  "material-icon-theme.folders.associations": {
    "infra": "app",
    "entities": "class",
    "domain": "class",
    "schemas": "class",
    "typeorm": "database",
    "repositories": "mappings",
    "http": "container",
    "migrations": "tools",
    "modules": "components",
    "implementations": "core",
    "dtos": "typescript",
    "fakes": "mock",
    "websockets": "pipe",
    "protos": "pipe",
    "grpc": "pipe",
    "providers": "include",
    "subscribers": "messages",
    "useCases": "controller",
    "kafka": "scripts",
    "mappers": "meta",
    "_shared": "shared",
    "eslint-config": "tools",
    "kube": "kubernetes",
    "reducers": "Redux-reducer",
    "actions": "Redux-actions",
    "store": "Redux-store",
    "redux": "Redux-actions",
    "storage": "Temp",
    "presentation": "Mjml",
    "exceptions": "Error",
    "exception": "Error",
    "filters": "Private",
    "filter": "Private"
  },

  "material-icon-theme.files.associations": {
    "index.ts": "Minecraft",
    "index.js": "Minecraft",
    "styles.ts": "Figma",

    "*.webpack.js": "webpack",
    //
    "*.reducer.js": "Redux-reducer",
    "reducer.js": "Redux-reducer",
    "*.reducer.ts": "Redux-reducer",
    "reducer.ts": "Redux-reducer",
    //
    "*.action.js": "Redux-action",
    "action.js": "Redux-action",
    "*.action.ts": "Redux-action",
    "action.ts": "Redux-action",
    //
    "*.store.js": "Redux-store",
    "store.js": "Redux-store",
    "*.store.ts": "Redux-store",
    "store.ts": "Redux-store",
    //
    "*.i18n.js": "I18n",
    "*.i18n.ts": "I18n",
    //
    "routes.js": "Routing",
    "router.js": "Routing",
    "*router.js": "Routing",
    "*router.ts": "Routing",
    "routes.ts": "Routing",
    "router.ts": "Routing",
    "*.routes.ts": "Routing",
    "*.router.ts": "Routing",
    //
    "types.ts": "typescript-def",
    "type.ts": "typescript-def",
    "*.types.ts": "typescript-def",
    "*.type.ts": "typescript-def"
  },
  "material-icon-theme.languages.associations": {
    "dotenv": "tune"
  },

  // Reassociate files wih the correct language
  "files.associations": {
    ".sequelizerc": "javascript",
    ".stylelintrc": "json",
    ".prettierrc": "json",
    "*.tsx": "typescriptreact",
    ".env.*": "dotenv",
    ".env*": "dotenv"
  },

  //------------------------------------------------------------------------------------------------
  //Extensions

  //inlineparameters
  "workbench.colorCustomizations": {
    "inlineparameters.annotationForeground": "#8be9fd",
    "inlineparameters.annotationBackground": "#ffffff00"
  },
  "inline-parameters.fontStyle": "normal",
  "inline-parameters.fontSize": "14px",

  //GitLens
  "gitlens.codeLens.recentChange.enabled": false,
  "gitlens.codeLens.authors.enabled": false,
  "gitlens.codeLens.enabled": false,

  //Colorize
  "colorize.include": ["*"],

  //TODO Tree
  "todo-tree.regex.regex": "(//|#|<!--|;|/\\*|^|^\\s*(-|\\d+.))\\s*($TAGS)",
  "todo-tree.general.tags": [
    "BUG",
    "HACK",
    "FIXME",
    "TODO",
    "XXX",
    "[ ]",
    "[x]",
    "TESTE"
  ],

  //PolarCode
  "polacode.backgroundColor": "#6633CC",

  //Template String Converter
  "template-string-converter.quoteType": "both",
  "template-string-converter.autoRemoveTemplateString": true,
  "template-string-converter.convertWithinTemplateString": true,
  "template-string-converter.addBracketsToProps": true,
  "template-string-converter.validLanguages": [
    "svelte",
    "typescript",
    "javascript",
    "typescriptreact",
    "javascriptreact"
  ],

  //Live Server
  "liveServer.settings.donotShowInfoMsg": true,

  //Code Spell Checker
  "cSpell.language": "en,pt,pt_BR",
  "cSpell.userWords": [
    "airbnb",
    "backoffice",
    "camelcase",
    "chakra",
    "Cnpj",
    "dayjs",
    "defaulticon",
    "draftjs",
    "gatsby",
    "hookform",
    "Iphone",
    "jwplayer",
    "lightbox",
    "lottie",
    "mailtrap",
    "middlewares",
    "minetoo",
    "nestjs",
    "nextjs",
    "ongoingorder",
    "pageview",
    "Parens",
    "plopfile",
    "pokemon",
    "recaptcha",
    "reduxjs",
    "screenfull",
    "storaged",
    "stylelint",
    "svgr",
    "sxsw",
    "SXSW",
    "tailwindcss",
    "tanstack",
    "theming",
    "toastify",
    "trpc",
    "typecheck",
    "uuidv",
    "videojs",
    "vitejs",
    "Youtube"
  ],

  //GitHub Copilot
  "github.copilot.enable": {
    "*": true,
    "plaintext": true,
    "markdown": true,
    "scminput": false,
    "yaml": false,
    "typescriptreact": true,
    "typescript": true
  },

  //Notes
  "Notes.notesLocation": "/home/recri/Documents/Notes",
  // random bellow
  "xml.java.home": null,
  "tabnine.experimentalAutoImports": true,
  "[xml]": {
    "editor.defaultFormatter": "DotJoshJohnson.xml"
  },
  "editor.accessibilitySupport": "off",
  "totalTypeScript.hideAllTips": false,
  "totalTypeScript.hideBasicTips": true,
  "totalTypeScript.hiddenTips": [
    "passing-generics-to-types",
    "keyof",
    "typeof",
    "non-null-expression",
    "type-alias-with-generics",
    "generic-slots-in-functions",
    "omit-utility-type",
    "partial-utility-type",
    "returntype-utility-type",
    "mapped-type",
    "record-utility-type",
    "pick-utility-type",
    "type-alias-with-multiple-generics",
    "tuple-type"
  ],
  "terminal.integrated.env.windows": {},
  "terminal.integrated.env.linux": {},
  "terminal.integrated.env.osx": {},
  "terminal.integrated.enableMultiLinePasteWarning": false,
  "remote.SSH.remotePlatform": {
    "Macbook": "macOS",
    "renans-mbp": "macOS"
  },
  "remote.SSH.connectTimeout": 30,
  "editor.tokenColorCustomizations": {
    "[*Light*]": {
      "textMateRules": [
        {
          "scope": "ref.matchtext",
          "settings": {
            "foreground": "#000"
          }
        }
      ]
    },
    "[*Dark*]": {
      "textMateRules": [
        {
          "scope": "ref.matchtext",
          "settings": {
            "foreground": "#fff"
          }
        }
      ]
    },
    "textMateRules": []
  },
  "dotenv.enableAutocloaking": false,
  "[prisma]": {
    "editor.defaultFormatter": "Prisma.prisma"
  },
  "figma.autocompleteBlocks": true,
  "tailwindCSS.experimental.classRegex": [
    ["tv\\(([^)]*)\\)", "[\"'`]([^\"'`]*).*?[\"'`]"]
  ],
  "console-ninja.featureSet": "Community",
  "[svelte]": {
    "editor.defaultFormatter": "svelte.svelte-vscode"
  },
  "remote.autoForwardPortsSource": "hybrid",
  "svelte.enable-ts-plugin": true
}
